@model IEnumerable<Microsoft.Graph.Message>

@{
    ViewBag.Current = "Mail";
}

<h1>Mail List</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Subject</th>
            <th scope="col">Message</th>
            <th scope="col">Body</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Subject</td>
            <td>@item.BodyPreview</td>
            <td>@Html.Raw(item.Body.Content)</td>
        </tr>
        }
    </tbody>
</table>